<h1>New user?</h1>
<p>Find me in app/views/users/new.html.erb</p>
<p>Sign up now!</p>

<%= form_for(@user) do |f| %>
  <div class="field" >
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  
  <div class="field" >
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  
  # <div class="field" >
   # <%= f.label :password %>
   # <%= f.text_field :password %>
  #</div>
  
  <div class="actions" >
    <%= f.submit "Sign up" %>
  </div>
<% end %>
